{
  "name": "timeframes",
  "files": [
    "lib/**/*"
  ],
  "version": "1.0.2",
  "description": "Utilities for dealing with timeframes",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boy51/timeframe.git"
  },
  "keywords": [
    "timeframes",
    "timespan"
  ],
  "author": "webnature solutions GmbH",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boy51/timeframe/issues"
  },
  "homepage": "https://github.com/boy51/timeframe#readme",
  "devDependencies": {
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "faker": "^5.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "moment": "^2.29.1"
  }
}
